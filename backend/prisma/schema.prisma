datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id        String      @id @default(cuid())
  name      String
  email     String   @unique
  mobile    String   @unique
  gender    String
  password  String
  createdAt DateTime @default(now())
}
